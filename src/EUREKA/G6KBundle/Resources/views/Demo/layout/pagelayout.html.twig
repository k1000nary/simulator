{# DO NOT EDIT THIS FILE #}	
{% extends 'EUREKAG6KBundle:base/layout:pagelayout.html.twig' %}

{% block title %}G6K Demo - {{ step.simulator.label}}{% endblock %}

{% block metas %}
{{ parent() }}<meta name="viewport" content="width=device-width, initial-scale=1.0" />
{% include 'EUREKAG6KBundle:Demo/sections:meta.html.twig' %}
{% endblock %}

{% block styles %}
{{ parent() }}
<link rel='stylesheet' type='text/css' href="{{ asset('bundles/eurekag6k/base/js/bootstrap/css/bootstrap.css') }}" />
{% include 'EUREKAG6KBundle:Demo/sections:favicon.html.twig' %}
{% include 'EUREKAG6KBundle:Demo/sections:styles.html.twig' %}
{% endblock %}

{% block body %}
	{% include 'EUREKAG6KBundle:Demo/sections:header.html.twig' %}
	<main class="main" id="main" role="main">
		<div class="container">
			{% block content %}{% endblock %}
		</div>
	</main>
	{% include 'EUREKAG6KBundle:Demo/sections:footer.html.twig' %}
	{% block scripts %}
		{{ parent() }}
		<script type="text/javascript">
		$(document).ready(function(){
			$('input.date').datepicker('theme', 'blue');
			$('input.listbox-input').listbox('theme', 'blue');	
		});
		</script>
		{% include 'EUREKAG6KBundle:Demo/sections:scripts.html.twig' %}
	{% endblock %}
{% endblock %}
